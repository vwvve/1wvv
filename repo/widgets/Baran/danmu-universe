function isValidJWT(e, t = null) {
    try {
        let n = e.split(".");
        if (3 !== n.length) return !1;
        let [r] = n;
        if (!r) return !1;
        let i = JSON.parse(atob(r));
        if ("typ" in i && i?.typ !== "JWT" || !i.alg || t && (!("alg" in i) || i.alg !== t)) return !1;
        return !0;
    } catch  {
        return !1;
    }
}
