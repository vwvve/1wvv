function base64ToUint8Array(e) {
    let t = enc_base64_default().parse(e), n = new Uint8Array(t.sigBytes);
    for(let e = 0; e < t.sigBytes; e++){
        let r = e >>> 2, i = e % 4;
        n[e] = t.words[r] >>> 24 - 8 * i & 0xff;
    }
    return n;
}
