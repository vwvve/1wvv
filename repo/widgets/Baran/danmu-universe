    cdataPropName: !1,
    numberParseOptions: {
        hex: !0,
        leadingZeros: !0,
        eNotation: !0
    },
    tagValueProcessor: function(e, t) {
        return t;
    },
    attributeValueProcessor: function(e, t) {
        return t;
    },
