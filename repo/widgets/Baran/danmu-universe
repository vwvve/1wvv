class YoukuScraper extends BaseScraper {
    get token() {
        var e, t;
        return (null != (t = null == (e = this.fetch.getCookie("_m_h5_tk")) ? void 0 : e.split("_")[0]) ? t : "").substring(0, 32);
    }
