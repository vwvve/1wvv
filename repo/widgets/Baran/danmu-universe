const defaultOptions = {
    preserveOrder: !1,
    attributeNamePrefix: '@_',
    attributesGroupName: !1,
    textNodeName: '#text',
    ignoreAttributes: !0,
    removeNSPrefix: !1,
    allowBooleanAttributes: !1,
    parseTagValue: !0,
    parseAttributeValue: !1,
