    }
    createSign(e) {
        let t = Object.keys(e).filter((e)=>"sign" !== e).sort().map((t)=>{
            var n;
            return `${t}=${null != (n = e[t]) ? n : ""}`;
        }).join("&");
        return md5_default()(`${t}&secret_key=howcuteitis`).toString().toUpperCase();
    }
    filterAndFinalizeEpisodes(e, t) {
        let n = this.getEpisodeBlacklistPattern();
