function isValidBase64URL(e) {
    if (!base64url.test(e)) return !1;
    let t = e.replace(/[-_]/g, (e)=>"-" === e ? "+" : "/");
    return isValidBase64(t.padEnd(4 * Math.ceil(t.length / 4), "="));
}
